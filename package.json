{
    "name": "sprout-bfee-blankrepo",
    "version": "0.0.1",
    "description": "Sprout template to create a blank repo.",
    "repository": "github:brennanfee/sprout-bfee-blankrepo",
    "homepage": "https://github.com/brennanfee/sprout-bfee-blankrepo#readme",
    "bugs": "https://github.com/brennanfee/sprout-bfee-blankrepo/issues",
    "author": "Brennan Fee <github@todaytechsoft.biz> (https://github.com/brennanfee)",
    "keywords": [
        "sprout-template",
        "bfee",
        "blankrepo"
    ],
    "devDependencies": {
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.0",
        "prettier": "^1.12.1",
        "sprout": "^1.2.1"
    },
    "dependencies": {
        "gh-got": "^7.0.0",
        "underscore.string": "^3.3.4"
    },
    "main": "init.js",
    "license": "Apache-2.0",
    "eslintConfig": {
        "env": {
            "node": true,
            "es6": true
        },
        "extends": [
            "plugin:prettier/recommended"
        ]
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid",
        "proseWrap": "always",
        "requirePragma": false,
        "insertPragma": false,
        "semi": false
    }
}
